pipeline {

	agent any 

	stages {

			stage('Build'){
				steps {
					echo "Testing Build.............!!"

				}

			}

			stage ('Test'){

				steps {
					echo "Testing Stage..............!!!"	

				}

			}

			stage ('Run My Script'){

				steps {
					echo "Running My Script..............!!!"	
				}

			}

			stage ('Deploy'){

				steps {
echo "Deployment completed...!!!"
				}

			}
	}

}		
